
#include <iostream>
using namespace std;

int main() {
    const int numStudents = 6; 
    int grades[numStudents];   

    
    for (int i = 0; i < numStudents; i++)
  {
        cout << "Enter grade for student " << i + 1 << ": ";
        cin >> grades[i];
    }

    
    int highest = grades[0]; 
    for (int i = 1; i < numStudents; i++)
  {
        if (grades[i] > highest)
      {
            highest = grades[i]; 
        }
    }
    cout << "The highest grade is: " << highest << endl;

    return 0;
}

