#include <iostream>
using namespace std;

int main() {
    int arr[5];  // array of 5 integers

    // Input from user
    cout << "Enter 5 numbers:" << endl;
    for (int i = 0; i < 5; i++) 
    {
        cin >> arr[i];
    }

    // Output all numbers entered
    cout << "You entered:" << endl;
    for (int i = 0; i < 5; i++)
    {
        cout << arr[i] << endl;
    }

    return 0;
}
